{
    "bindings": [
        {
            "name": "Timer",
            "type": "timerTrigger",
            "direction": "in",
            "schedule": "0 */10 * * * *"
        }
    ],
    "disabled": false,
    "singletonLock": {
        "lock": {
            "mode": "Exclusive"
        }
    },
    "concurrency": {
        "maxConcurrentCalls": 1,
        "singleInstance": true
    }
}